#! /usr/bin/env node

var commands = [
      'server',
      'test'
    ],
    command = process.argv[2] || '<none given>';


if(commands.indexOf(command) === -1){
  console.log(
    "command: %s is not found. Available commands:", 
    command,
    commands.join(', ')
  );
} else {
  require('../lib/node/bin/' + command);
}
